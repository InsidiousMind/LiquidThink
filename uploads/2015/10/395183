    /**
     * GPT responsive version.
     */
    (function(){
        var sonobiSizes = {
            "300x250" : "5f2d6bce9df694672d0c",
            "300x600" : "4449390ce636c2bba09c",
            "728x90" : "0848bd941032b63fc7c7",
            "970x250" : "7d140ce0d19eb6068341"
        };

        var size, ids, sizeIn, id;
        for (var key in sonobiSizes) {
            size = key.split("x");
            coded_size = sonobiSizes[key];
            sizeIn = $("gpt-sizeset[sizes*='[" + size[0] + ", " + size[1] + "]']");
            ids = $.map(sizeIn, function(el) { 
                return el.parentElement.id;
            });
            for (var i = 0; i < ids.length; i++) {
                id = ids[i];
                sbi_morpheus.register(id, coded_size);
            }
        }

    }());
</script>

