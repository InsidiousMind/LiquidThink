<script type="text/javascript">
    Atlantic.ad_queue = Atlantic.ad_queue || [];
    Atlantic.ad_queue.push(function(){
        Atlantic.Ad.sizeListener(/^welcome/, function(pos, size){
            if (!size) {
                return false;
            }
            var $lightbox = $('#lightbox-welcome');

            // The lightbox's class includes the size, so ipad
            // ads and other weird cases can have their own styling.
            $lightbox.addClass("active format-" + size.join("x"));
            if (typeof window.welcomeScreen === 'undefined') {
                window.welcomeScreen = new Atlantic.WelcomeScreen();
            }
            window.welcomeScreen.show();

            // Clicking elsewhere on the page will close the iframe
            // after 1 second.
            setTimeout(function() {
                $(document).on("click", function(e) {
                    if (!$.contains($("#ad_")[0], e.target)) {
                        welcomeScreen.hide();
                    }
                });
            }, 1500);
        });
    });
</script>
