<script type="text/javascript">
    //Impression Tracking
    var impression_map = {};
    var evar_map = {};
    if (impression_data.length > 0) {
                            
        
        for (var i = 0; i < impression_data.length; i++) {
            var zoneType = impression_data[i].eVar49.zoneType;
            var Position = impression_data[i].eVar49.position;
            var moduleType = impression_data[i].moduleType;
            var moduleName = impression_data[i].moduleName;

            //Click values are different because prop3 isn't available as a dynamic var
            var eVar49Click = zoneType + '--'
                + Position + '--'
                + '"+pageName+"' + '--'
                + moduleName + '--'
                + '"+v74';
            var impressionClick = ';' + moduleType + '--'
                + moduleName + ';;;;'
                + 'eVar49=' + eVar49Click;

            var eVar49 = zoneType + '--'
                + Position + '--'
                + '"+pageName+"' + '--'
                + moduleName + '--'
                + '"+c3';
