  <div class="search-menu-inner">

    <div class="search-branding-mod">
      Star Tribune
    </div><!--eo search-branding-mod-->
    <script>
    	  $(document).ready(function () {
    	  
    	  	var radioFilter = $(".search-menu-radio-group").find("input:radio[name=searchType]");

    	  	radioFilter.change(function(){
    	  		if($(this).val() === 'businessSearch'){
                   $(this).parent().parent().parent().find(".search-menu-form").attr("action", "http://local.startribune.com/search");
           		} else { 
           			$(this).parent().parent().parent().find(".search-menu-form").attr("action", "/search/");
           		}
    	  	});
        });
    </script>
    		<form class="search-menu-form" action="/search/" method="get">
			<div class="search-menu-input-group">
				<input class="search-menu-input" id="search" name="keyword" type="text" value="" placeholder="Enter a search term"/>
				<input class="search-menu-submit" type="submit" value="Submit"/>
			</div><!--eo search-menu-input-group-->
						<div class="search-menu-radio-group">
				<input type="radio" checked name="searchType" id="filter-all-content" value="allContent">
				<label class="search-menu-radio-label first" for="filter-all-content">All content</label>
				<input type="radio" name="searchType" id="filter-business-listings" value="businessSearch">
				<label class="search-menu-radio-label last" for="filter-business-listings">Business listings</label>
			</div><!--eo search-menu-radio-group-->
					</form><!--eo search-menu-form-->
		    

    <a href="#" class="search-menu-close-trigger js-close-search-menu">Close</a>

  </div><!--eo search-menu-inner-->
</nav><!--eo search-menu-mod -->
